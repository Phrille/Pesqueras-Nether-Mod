{
    "multipart": [
        {   "apply": { "model": "pesquerasnethermod:netherroot_post" }},
        {   "when": { "north": "true" },
            "apply": { "model": "pesquerasnethermod:netherroot_side", "uvlock": true }
        },
        {   "when": { "east": "true" },
            "apply": { "model": "pesquerasnethermod:netherroot_side", "y": 90, "uvlock": true }
        },
        {   "when": { "south": "true" },
            "apply": { "model": "pesquerasnethermod:netherroot_side", "y": 180, "uvlock": true }
        },
        {   "when": { "west": "true" },
            "apply": { "model": "pesquerasnethermod:netherroot_side", "y": 270, "uvlock": true }
        },
        {   "when": { "up": "true" },
            "apply": { "model": "pesquerasnethermod:netherroot_top", "uvlock": true }
        },
        {   "when": { "down": "true" },
            "apply": { "model": "pesquerasnethermod:netherroot_bottom", "uvlock": true }
        }
    ]
}
